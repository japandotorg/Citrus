{
    "name": "discord-citrus",
    "version": "1.0.1",
    "description": "The next big discord bot framework for Discord.js",
    "main": "index.js",
    "scripts": {
        "lint": "yarn eslint ./src",
		"test": "yarn build && cd ./test && yarn && yarn test && cd ../",
		"test:run": "yarn build && cd ./test && yarn && yarn start && cd ../",
		"test:all": "yarn lint && yarn test",
		"format": "yarn prettier . --write",
		"build": "yarn rimraf dist && yarn tsc",
		"build:no-emit": "yarn tsc --noEmit",
		"prepare": "yarn build"
    },
    "keywords": [
        "discord",
        "discord-js",
        "discord.js",
        "framework",
        "commands",
        "bot-framework",
        "modular",
        "customizable"
    ],
    "author": "Lemon Rose <yash.kul69@gmail.com> [https://japandotorg.me]",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/japandotorg/Citrus"
    },
    "bugs": {
        "url": "https://github.com/japandotorg/Citrus/issues"
    },
    "homepage": "https://github.com/japandotorg/Citrus",
    "prettier": {
        "useTabs": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "printWidth": 130,
        "overrides": [
            {
                "files": "*.md",
                "options": {
                    "useTabs": false
                }
            }
        ]
    },
    "dependencies": {
        "@discordjs/voice": "github:discordjs/voice",
        "discord-api-types": "^0.26.1",
        "discord.js": "^14.0.0-dev.1642723600.c10afea",
        "source-map-support": "^0.5.21"
    },
    "devDependencies": {
        "@favware/npm-deprecate": "^1.0.4",
        "@types/node": "^17.0.10",
        "@types/source-map-support": "^0.5.4",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-deprecation": "^1.3.2",
        "jsdoc": "^3.6.7",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.5"
    }
}
